{
  "name": "wordium",
  "root": "./dist",
  "prerender": true,
  "routes": {
    "/**": "index.html"
  },
  "proxy": {
    "data": {
      "origin": "https://github.com/wordtalk/wordium/ or http://twubs.com/wordtalk",
      "cookies": false,
      "headers": {
        "content-type": "text/javascript"
      },
      "timeout": 30
    }
  }
}
